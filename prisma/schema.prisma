generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Game {
  id          String   @id @default(uuid())
  createdAt   DateTime @default(now())
  mode        String
  fen         String
  moves       String[]
  status      String
  winner      String?
  playerWhite String?
  playerBlack String?
}
